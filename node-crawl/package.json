{
  "name": "appointed-server",
  "version": "0.0.1",
  "description": "appointed-server",
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "scripts": {
    "start": "nodemon --watch src --watch src/**/*.ts --exec \"npm run eslint && npm run dev\" ",
    "build": "tsc && tsc-alias && npm run createLogs",
    "createLogs": "mkdir ./dist/src/logs && mkdir ./dist/src/logs/allLogs  && mkdir ./dist/src/logs/errorLogs",
    "pre-commit": "lint-staged",
    "lint-fix": "eslint --fix .",
    "dev": "ts-node -r tsconfig-paths/register --files src/index.ts",
    "eslint": "./node_modules/.bin/eslint ./src"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "axios-https-proxy-fix": "^0.17.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "gm": "^1.23.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.8",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.31.0",
    "tesseract.js": "^3.0.2",
    "tunnel": "^0.0.6",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.14",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.6",
    "@types/sharp": "^0.30.5",
    "@types/tunnel": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "ts-loader": "^9.3.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  }
}
